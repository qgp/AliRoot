# Syntax: revision numbers x y z preceded by key '---' and module/comment
#
#   revision log: '--- [module/comment]: x y z'
#   merging:      '---> [module/comment]: x y z'
#
# merging of an entry of this file:
# cat $ALICE_ROOT/HLT/ChangeLog | sed -e '/^#/d' -e '/--->/!d' | while read line; do revisions=`echo $line | sed -e 's|---> [ A-Za-z/]*:||'`; text=`echo $line | sed -e 's|:[ 0-9]*||g'`; for r in $revisions; do echo $r $text; svn merge -c $r https://alisoft.cern.ch/AliRoot/trunk;done; done
#
# checking log message and diff of the selected revisions
# cat $ALICE_ROOT/HLT/ChangeLog | sed -e '/^#/d' -e '/--->/!d' | while read line; do revisions=`echo $line | sed -e 's|---> [ A-Za-z/]*:||'`; text=`echo $line | sed -e 's|:[ 0-9]*||g'`; for r in $revisions; do echo $r $text;svn log -c $r https://alisoft.cern.ch/AliRoot/trunk/HLT; svn diff -c $r https://alisoft.cern.ch/AliRoot/trunk;done; done
#
# comparing modified files with another aliroot version
# for f in `svn st HLT | grep Ali | sed -e 's|^MM*||'`; do echo $f; diff -I '$Id' $f ../AliRoot_HEAD_2012-04-04/$f; done
#
# merged revisions:
# for r in `cat $ALICE_ROOT/HLT/ChangeLog | sed -e '/^#/d' -e '/---/!d' -e 's|---[ >/:A-Za-z]*||'`; do echo $r; done | sort
#
# changes in the trunk since creation of the branch
# svn log -r 50671:HEAD https://alisoft.cern.ch/AliRoot/trunk/HLT/ | sed -e '/^r[0-9]/!d' -e 's|^r||' | cut -d ' ' -f 1
#
# check differences and sort
# choice=port;for f in diff_*.diff; do kompare $f; echo port $f? "[$choice], n"; folder=$choice; read choice; if test "x$choice" == "x"; then choice=$folder; fi; if test "x$choice" != "xn"; then mkdir -p $choice;mv $f $choice; else choice=$folder; fi; done
